\n        userInfo && generateOrder()\r\n    }, [userInfo])\r\n\r\n    useEffect(() => {\r\n        orderId && updateStock()\r\n    },[orderId])\r\n\r\n\r\n    return (\r\n        <div>\r\n            <CartForm getInfo={getInfo} orderId={orderId} setShowForm={setShowForm} />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CartOrder\r\n","C:\\Users\\Brian\\Desktop\\BRIAN\\React CoderHouse\\Ecommerce\\ecommerce-lion\\src\\components\\CartForm\\CartForm.jsx",[],"C:\\Users\\Brian\\Desktop\\BRIAN\\React CoderHouse\\Ecommerce\\ecommerce-lion\\src\\components\\orderModal\\OrderModal.jsx",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"145","replacedBy":"149"},{"ruleId":"147","replacedBy":"150"},{"ruleId":"151","severity":1,"message":"152","line":9,"column":13,"nodeType":"153","messageId":"154","endLine":9,"endColumn":17},{"ruleId":"145","replacedBy":"155"},{"ruleId":"147","replacedBy":"156"},{"ruleId":"145","replacedBy":"157"},{"ruleId":"147","replacedBy":"158"},{"ruleId":"151","severity":1,"message":"159","line":11,"column":13,"nodeType":"153","messageId":"154","endLine":11,"endColumn":17},{"ruleId":"151","severity":1,"message":"160","line":1,"column":43,"nodeType":"153","messageId":"154","endLine":1,"endColumn":52},{"ruleId":"145","replacedBy":"161"},{"ruleId":"147","replacedBy":"162"},{"ruleId":"163","severity":1,"message":"164","line":53,"column":8,"nodeType":"165","endLine":53,"endColumn":18,"suggestions":"166"},{"ruleId":"163","severity":1,"message":"167","line":57,"column":7,"nodeType":"165","endLine":57,"endColumn":16,"suggestions":"168"},"no-native-reassign",["169"],"no-negated-in-lhs",["170"],["169"],["170"],"no-unused-vars","'cart' is assigned a value but never used.","Identifier","unusedVar",["169"],["170"],["169"],["170"],"'user' is assigned a value but never used.","'useEffect' is defined but never used.",["169"],["170"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'generateOrder'. E