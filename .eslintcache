Stock = [];\r\n            query.docs.forEach((docSnapshot, index) => {\r\n                if(docSnapshot.data().stock >= cart[index].quantity) {\r\n                    batch.update(docSnapshot.ref, { stock: docSnapshot.data().stock - cart[index].quantity })\r\n                } else {\r\n                    outOfStock.push({...docSnapshot.data(), id: docSnapshot.id})\r\n                }\r\n            })\r\n            outOfStock.length === 0 && await batch.commit()\r\n        })()\r\n    }\r\n\r\n    useEffect(() => {\r\n        userInfo && generateOrder()\r\n    }, [userInfo])\r\n\r\n    useEffect(() => {\r\n        orderId && updateStock()\r\n    },[orderId])\r\n\r\n\r\n    return (\r\n        <div>\r\n            <CartForm getInfo={getInfo} orderId={orderId} setShowForm={setShowForm} />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CartOrder\r\n","C:\\Users\\Brian\\Desktop\\BRIAN\\React CoderHouse\\Ecommerce\\ecommerce-lion\\src\\components\\CartForm\\CartForm.jsx",[],"C:\\Users\\Brian\\Desktop\\BRIAN\\React CoderHouse\\Ecommerce\\ecommerce-lion\\src\\components\\orderModal\\OrderModal.jsx",[],"C:\\Users\\Brian\\Desktop\\BRIAN\\React CoderHouse\\Ecommerce\\ecommerce-lion\\src\\components\\404NotFound\\NotFound.jsx",[],{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"135","replacedBy":"139"},{"ruleId":"137","replacedBy":"140"},{"ruleId":"135","replacedBy":"141"},{"ruleId":"137","replacedBy":"142"},{"ruleId":"143","severity":1,"message":"144","line":11,"column":13,"nodeType":"145","messageId":"146","endLine":11,"endColumn":17},{"ruleId":"135","replacedBy":"147"},{"ruleId":"137","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":53,"column":8,"nodeType":"151","endLine":53,"endColumn":18,"suggestions":"152"},{"ruleId":"149","severity":1,"message":"153","line":57,"column":7,"nodeType":"151","endLine":57,"endColumn":16,"suggestions":"154"},"no-native-reassign",["155"],"no-negated-in-lhs",["156"],["155"],